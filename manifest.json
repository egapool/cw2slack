{
  "manifest_version": 2,
  "name": "cw2slack",
  "version": "1.0",
  "description": "UI convertor ChatWork to slack",
  // "page_action": {
  //   "icon": "icon.png",
  //   "default_title": "cw2slack"
  // },
  "content_scripts" : [
    {
      "matches": ["https://www.chatwork.com/*"],
      "js":["js/my.js"]
    }
  ],
  "web_accessible_resources": ["css/style.css"],
  "permissions": [
    "tabs",
    "https://www.chatwork.com/*",
    "http://*/",
    "https://*/"
  ],
  // "background": {
  //   "scripts":["js/background.js"]
  //   },
  "icons" : {
    "128": "icon.png"
  }
}